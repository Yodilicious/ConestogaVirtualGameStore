@model IEnumerable<ConestogaVirtualGameStore.Web.Models.ApplicationUser>

@{
    ViewData["Title"] = "Index";
}

<h2>User Profiles</h2>

<table class="table">
    <thead>
        <tr>
            <th>
                User Profile Name
            </th>
                
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                <a asp-action="Details" asp-route-id="@item.Id">Details</a>
            </td>
        </tr>
}
    </tbody>
</table>
